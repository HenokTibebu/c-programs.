    //declaration and initialization
    char a;//%c
    char name[] = {};//%s
    int age ;//%d
    float area ;//%f
    double salary ;//%lf
    
    printf("Enter a character:");
    scanf("%c",&a);
    printf("Enter your name:");
    scanf("%s",&name);
    
    printf("Enter your age:");
    scanf("%d",&age);
    
    printf("Enter area:");
    scanf("%f",&area);
    
    printf("Enter the salary:");
    scanf("%lf",&salary);
    printf("        \n");
    
    
    printf("The character is %c\n",a);
    printf(" \n");
    printf("The string is %s\n",name);
    printf(" \n");
    printf("The age is %d years\n",age);
    printf(" \n");
    printf("The area is %.1f square cm\n",area);
    printf(" \n");
    printf("The salary is %.2lf ksh\n",salary);
    printf(" \n");
    
    return 0;
}
 
 
